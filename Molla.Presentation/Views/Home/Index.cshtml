                 data-owl-options='{"nav": false, "rtl": true}'>
                
                @foreach(var slide in Model.Slider)
                {
                    <div class="intro-slide">
                        <figure class="slide-image">

                            <picture>
                                <source media="(max-width: 480px)"
                                        srcset="@slide.ImageSource">
                                <img src="@slide.ImageSource" alt="Image Desc">
                            </picture>
                        </figure><!-- End .slide-image -->

                        <div class="intro-content">
                            <h3 class="intro-subtitle">@slide.Title</h3><!-- End .h3 intro-subtitle -->
                            <h1 class="intro-title text-white">@slide.Description</h1>
                            <!-- End .intro-title -->

                            <div class="intro-price text-white">@slide.Tag</div><!-- End .intro-price -->

                            <a href=@slide.Link class="btn btn-white-primary btn-round">
                                <span>خرید</span>
                                <i class="icon-long-arrow-left"></i>
                            </a>
                        </div><!-- End .intro-content -->
                    </div>

                    <!-- End .intro-slide -->
                }

                

                
